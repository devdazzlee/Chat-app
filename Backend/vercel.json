{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/register", "dest": "/index.js" },
      { "src": "/api/login", "dest": "/index.js" },
      { "src": "/api/logout", "dest": "/index.js" },
      { "src": "/api/profile", "dest": "/index.js" },
      { "src": "/api/users", "dest": "/index.js" },
      { "src": "/api/messages", "dest": "/index.js" },
      { "src": "/api/messages/:userId", "dest": "/index.js" },
      { "src": "/api/messages/group", "dest": "/index.js" },
      { "src": "/api/groups", "dest": "/index.js" },
      { "src": "/api/groups/leave", "dest": "/index.js" },
      { "src": "/api/groups/:groupId", "dest": "/index.js" },
      { "src": "/process-payment", "dest": "/index.js" }
    ]
  }
  