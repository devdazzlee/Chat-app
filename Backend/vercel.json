{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.js"
      }
    ],
    "env": {
      "JWT_SECRET": "@jwt_secret",
      "MONGO_URI": "@mongo_uri",
      "PORT": "5000"
    }
  }
  